<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="header" maxHeight="75.0" minHeight="75.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ovapp.controllers.layout.NavbarController">
    <children>
        <VBox fx:id="headerHamburger" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <ImageView fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../../assets/icons/more.png" />
                    </image>
                </ImageView>
            </children>
        </VBox>
        <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
            <children>

                <ImageView fitHeight="90.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../../assets/brand/train.png" />
                    </image>
                </ImageView>
                <Label style="-fx-text-fill: #F4F3EE; -fx-font-weight: bold; -fx-font-family: 'Verdana'; -fx-font-size: 19;" text="Hoofdpagina">
                    <font>
                        <Font name="Verdana" size="31.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <VBox prefHeight="200.0" prefWidth="100.0" />
    </children>
</HBox>
